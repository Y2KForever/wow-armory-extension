{
  "name": "armory-extension-wow",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "release": "yarn build && ./scripts/zip.sh && yarn sync && yarn invalidate",
    "sync": "aws s3 sync ./dist s3://wow-extension-landing-page --exclude \"*\" --include \"landing.html\" --include \"assets/landing-*\" --include \"assets/main-*\"",
    "invalidate": "aws cloudfront create-invalidation --distribution-id ECKCFNPG4IIL --paths \"/*\"",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.1.5",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@reduxjs/toolkit": "^2.3.0",
    "@tanstack/react-table": "^8.21.2",
    "@types/react-window": "^1.8.8",
    "@uidotdev/usehooks": "^2.4.1",
    "@uiw/react-color": "^2.3.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.2.0",
    "lucide-react": "^0.469.0",
    "motion": "^12.2.0",
    "next-themes": "^0.4.4",
    "react": "^18.3.1",
    "react-day-picker": "8.10.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-virtualized": "^9.22.6",
    "react-virtualized-auto-sizer": "^1.0.25",
    "react-window": "^1.8.11",
    "simplebar-react": "^3.3.0",
    "sonner": "^1.7.2",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "usehooks-ts": "^3.1.0",
    "vite-plugin-rewrite": "^0.0.8"
